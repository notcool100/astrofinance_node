{
  "name": "astrofinance-backend",
  "version": "1.0.0",
  "description": "Backend for AstroFinance Financial Management System",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "prisma:seed": "ts-node src/prisma/seed.ts",
    "check:navigation": "ts-node src/scripts/check-navigation.ts",
    "seed:navigation": "ts-node src/scripts/seed-navigation.ts",
    "fix:navigation": "ts-node src/scripts/fix-navigation.ts"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^2.0.1",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.14.0",
    "uuid": "^11.1.0",
    "winston": "^3.8.2",
    "yup": "^1.6.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.7",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
